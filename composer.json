        "aura/sql":"~2.0",
        "aura/asset-bundle": "~2.0",
        "aura/web-kernel": "~2.0",
        "kilte/pagination": "~1.0",
        "foa/filter-input-bundle": "~1.1",
        "foa/filter-intl-bundle": "~1.1",
        "foa/html-view-bundle": "~2.0",
        "foa/domain-payload": "~1.0",
        "foa/responder-bundle": "~0.2"
    },
    "license": "BSD-2-Clause",
